{
  "name": "@offbit-ai/zeal-embed-sdk",
  "version": "1.0.2",
  "description": "Zeal Embed SDK for integrating workflow editor into web applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "publish:patch": "./scripts/publish.sh patch",
    "publish:minor": "./scripts/publish.sh minor",
    "publish:major": "./scripts/publish.sh major",
    "publish:beta": "./scripts/publish.sh prerelease --preid beta"
  },
  "keywords": [
    "zeal",
    "workflow",
    "embed",
    "iframe",
    "integration",
    "sdk"
  ],
  "author": "Zeal Team",
  "license": "Apache-2.0",
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/offbit-ai/zeal.git",
    "directory": "packages/zeal-embed-sdk"
  }
}

{
  "name": "zeal",
  "version": "1.0.0",
  "description": "Integrated Flow-based Programming Editor and Debugger",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run crdt:dev\" \"next dev\"",
    "serve": "next dev",
    "build": "npm run crdt:build && npm run templates:ingest && npm run graphrag:build && next build",
    "build:production": "NEXT_PUBLIC_DISABLE_CONSOLE_LOGS=true npm run build",
    "start": "concurrently \"npm run crdt:start\" \"next start\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen localhost:3000",
    "test:all": "npm run lint && npm run typecheck && npm run test",
    "templates:ingest": "node scripts/ingest-templates.js",
    "templates:ingest:force": "node scripts/ingest-templates.js --force",
    "graphrag:build": "node scripts/build-graphrag.js",
    "graphrag:build:force": "node scripts/build-graphrag.js --force",
    "postinstall": "npm run templates:ingest && npm run graphrag:build",
    "crdt:build": "node scripts/build-rust-server.js --release",
    "crdt:dev": "cd crdt-server && cargo run -- --port 8080 --verbose",
    "crdt:start": "cd crdt-server && cargo run --release -- --port 8080",
    "crdt:check": "node scripts/build-rust-server.js",
    "crdt:test": "cd crdt-server && cargo test",
    "crdt:fmt": "cd crdt-server && cargo fmt",
    "crdt:clippy": "cd crdt-server && cargo clippy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/offbit-ai/zeal.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/offbit-ai/zeal/issues"
  },
  "homepage": "https://github.com/offbit-ai/zeal#readme",
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@executeautomation/playwright-mcp-server": "^1.0.6",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/chokidar": "^1.7.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.863.0",
    "@aws-sdk/s3-request-presigner": "^3.863.0",
    "@azure/storage-blob": "^12.28.0",
    "@google-cloud/storage": "^7.17.0",
    "@supabase/supabase-js": "^2.53.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/glob": "^8.1.0",
    "@types/highlight.js": "^9.12.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/prismjs": "^1.26.5",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "glob": "^11.0.3",
    "graphology": "^0.26.0",
    "graphology-components": "^1.5.4",
    "graphology-shortest-path": "^2.1.0",
    "graphology-traversal": "^0.3.1",
    "highlight.js": "^11.11.1",
    "jsonpath-plus": "^10.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "pg": "^8.16.3",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-simple-code-editor": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "simple-icons": "^15.7.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "y-indexeddb": "^9.0.12",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.27",
    "zustand": "^5.0.6"
  }
}
